;; integer comparisons

{1 1 eq?} assert-true
{1 2 eq?} assert-false
{2 1 eq?} assert-false

{1 1 ne?} assert-false
{2 1 ne?} assert-true
{1 2 ne?} assert-true

{1 1 lt?} assert-false
{1 2 lt?} assert-true
{2 1 lt?} assert-false

{1 1 le?} assert-true
{1 2 le?} assert-true
{2 1 le?} assert-false

{1 1 gt?} assert-false
{1 2 gt?} assert-false
{2 1 gt?} assert-true

{1 1 ge?} assert-true
{1 2 ge?} assert-false
{2 1 ge?} assert-true

;; character comparisons

{"1" "1" eq?} assert-true
{"1" "2" eq?} assert-false
{"2" "1" eq?} assert-false

{"1" "1" ne?} assert-false
{"2" "1" ne?} assert-true
{"1" "2" ne?} assert-true

{""  "1" lt?} assert-true
{"1" "1" lt?} assert-false
{"1" "2" lt?} assert-true
{"2" "1" lt?} assert-false

{""  "1" le?} assert-true
{"1" "1" le?} assert-true
{"1" "2" le?} assert-true
{"2" "1" le?} assert-false

{""  "1" gt?} assert-false
{"1" "1" gt?} assert-false
{"1" "2" gt?} assert-false
{"2" "1" gt?} assert-true

{""  "1" ge?} assert-false
{"1" "1" ge?} assert-true
{"1" "2" ge?} assert-false
{"2" "1" ge?} assert-true

;; bool comparisons
{false false eq?} assert-true
{false true  eq?} assert-false
{true  false eq?} assert-false
{true  true  eq?} assert-true

{false false ne?} assert-false
{false true  ne?} assert-true
{true  false ne?} assert-true
{true  true  ne?} assert-false

{false false lt?} assert-false
{false true  lt?} assert-true
{true  false lt?} assert-false
{true  true  lt?} assert-false

{false false le?} assert-true
{false true  le?} assert-true
{true  false le?} assert-false
{true  true  le?} assert-true

{false false gt?} assert-false
{false true  gt?} assert-false
{true  false gt?} assert-true
{true  true  gt?} assert-false

{false false ge?} assert-true
{false true  ge?} assert-false
{true  false ge?} assert-true
{true  true  ge?} assert-true

;; bool
{false false and} assert-false
{false true  and} assert-false
{true  false and} assert-false
{true  true  and} assert-true

{false false or} assert-false
{false true  or} assert-true
{true  false or} assert-true
{true  true  or} assert-true

{false not} assert-true
{true  not} assert-false
