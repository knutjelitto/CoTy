;; integer comparisons

(1 1 eq?) assert-true
(1 2 eq?) assert-false
(2 1 eq?) assert-false

(1 1 ne?) assert-false
(2 1 ne?) assert-true
(1 2 ne?) assert-true

(1 1 < ) assert-false
(1 2 < ) assert-true
(2 1 < ) assert-false

(1 1 <=) assert-true
(1 2 <=) assert-true
(2 1 <=) assert-false

(1 1 > ) assert-false
(1 2 > ) assert-false
(2 1 > ) assert-true

(1 1 >=) assert-true
(1 2 >=) assert-false
(2 1 >=) assert-true

;; character comparisons

("1" "1" eq?) assert-true
("1" "2" eq?) assert-false
("2" "1" eq?) assert-false

("1" "1" ne?) assert-false
("2" "1" ne?) assert-true
("1" "2" ne?) assert-true

(""  "1" < ) assert-true
("1" "1" < ) assert-false
("1" "2" < ) assert-true
("2" "1" < ) assert-false

(""  "1" <=) assert-true
("1" "1" <=) assert-true
("1" "2" <=) assert-true
("2" "1" <=) assert-false

(""  "1" > ) assert-false
("1" "1" > ) assert-false
("1" "2" > ) assert-false
("2" "1" > ) assert-true

(""  "1" >=) assert-false
("1" "1" >=) assert-true
("1" "2" >=) assert-false
("2" "1" >=) assert-true

;; bool comparisons
(false false eq?) assert-true
(false true  eq?) assert-false
(true  false eq?) assert-false
(true  true  eq?) assert-true

(false false ne?) assert-false
(false true  ne?) assert-true
(true  false ne?) assert-true
(true  true  ne?) assert-false

(false false < ) assert-false
(false true  < ) assert-true
(true  false < ) assert-false
(true  true  < ) assert-false

(false false <=) assert-true
(false true  <=) assert-true
(true  false <=) assert-false
(true  true  <=) assert-true

(false false > ) assert-false
(false true  > ) assert-false
(true  false > ) assert-true
(true  true  > ) assert-false

(false false >=) assert-true
(false true  >=) assert-false
(true  false >=) assert-true
(true  true  >=) assert-true

;; bool
(false false and) assert-false
(false true  and) assert-false
(true  false and) assert-false
(true  true  and) assert-true

(false false or) assert-false
(false true  or) assert-true
(true  false or) assert-true
(true  true  or) assert-true

(false not) assert-true
(true  not) assert-false
