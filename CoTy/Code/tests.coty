;; integer comparisons

(1 1 ==) assert-true
(1 2 ==) assert-false
(2 1 ==) assert-false

(1 1 !=) assert-false
(2 1 !=) assert-true
(1 2 !=) assert-true

(1 1 < ) assert-false
(1 2 < ) assert-true
(2 1 < ) assert-false

(1 1 <=) assert-true
(1 2 <=) assert-true
(2 1 <=) assert-false

(1 1 > ) assert-false
(1 2 > ) assert-false
(2 1 > ) assert-true

(1 1 >=) assert-true
(1 2 >=) assert-false
(2 1 >=) assert-true

;; character comparisons

("1" "1" ==) assert-true
("1" "2" ==) assert-false
("2" "1" ==) assert-false

("1" "1" !=) assert-false
("2" "1" !=) assert-true
("1" "2" !=) assert-true

(""  "1" < ) assert-true
("1" "1" < ) assert-false
("1" "2" < ) assert-true
("2" "1" < ) assert-false

(""  "1" <=) assert-true
("1" "1" <=) assert-true
("1" "2" <=) assert-true
("2" "1" <=) assert-false

(""  "1" > ) assert-false
("1" "1" > ) assert-false
("1" "2" > ) assert-false
("2" "1" > ) assert-true

(""  "1" >=) assert-false
("1" "1" >=) assert-true
("1" "2" >=) assert-false
("2" "1" >=) assert-true

;; bool comparisons
(false false ==) assert-true
(false true  ==) assert-false
(true  false ==) assert-false
(true  true  ==) assert-true

(false false !=) assert-false
(false true  !=) assert-true
(true  false !=) assert-true
(true  true  !=) assert-false

(false false < ) assert-false
(false true  < ) assert-true
(true  false < ) assert-false
(true  true  < ) assert-false

(false false <=) assert-true
(false true  <=) assert-true
(true  false <=) assert-false
(true  true  <=) assert-true

(false false > ) assert-false
(false true  > ) assert-false
(true  false > ) assert-true
(true  true  > ) assert-false

(false false >=) assert-true
(false true  >=) assert-false
(true  false >=) assert-true
(true  true  >=) assert-true

